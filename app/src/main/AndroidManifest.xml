<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aasv11">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_aas"
        android:label="Anira"
        android:roundIcon="@drawable/logo_aas"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".SplashScreens.SplashScreenAdmin" ><intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>

        <activity
            android:name=".Admin.Admin"
            android:configChanges="orientation|screenSize" />
        <activity android:name=".Lecturer.Lecturer"
            android:screenOrientation="portrait"/>
        <activity android:name=".Student.Student"
            android:screenOrientation="portrait"/>
        <activity android:name=".Login.Login"
            android:screenOrientation="portrait">
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>